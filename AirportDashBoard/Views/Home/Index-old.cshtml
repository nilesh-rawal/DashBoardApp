<script type="text/javascript">

    $(document).ready(function () {
        LoadLolipopChart();
        GetKPI2L1();
        $("#LolipopFrequency").change(function () {
            LoadLolipopChart();
        });
        $("#slotadherence").change(function () {
            GetKPI2L1();
        });
    });

    function onclickpop() {
        window.location = "/Home/KPI"
    }

    var trafficHandled_Frequency = 'Hourly';
    //Create KPI2 L1
    function LoadKPI2() {
        $('.chart').easyPieChart({
            easing: 'easeOutBounce',
            onStep: function (from, to, percent) {
                $(this.el).find('.percent').text(Math.round(percent));
            },
            size: 110,
            // animate: 2000,
            lineCap: 'butt',
            scaleColor: false,
            barColor: '#17DEFF',
            trackColor: "rgba(255, 255, 255, 0.4)",
            lineWidth: 2
        });
    }

    $(function () {
        $('.chartSmall').easyPieChart({
            easing: 'easeOutBounce',
            onStep: function (from, to, percent) {
                $(this.el).find('.percentSmall').text(Math.round(percent));
            },
            size: 80,
            // animate: 2000,
            lineCap: 'butt',
            scaleColor: false,
            barColor: '#17DEFF',
            trackColor: "rgba(255, 255, 255, 0.7)",
            lineWidth: 2
        });
        // var chart = window.chart = $('.chart').data('easyPieChart');
        // $('.js_update').on('click', function() {
        //  chart.update(Math.random()*200-100);
        // });
    });



    //Create KPI1 L1
    function LoadLolipopChart() {
        trafficHandled_Frequency = $('#LolipopFrequency').find(":selected").text();
        $.ajax({
            url: "/Home/GetTrafficHandledJson",
            data: "Frequency=" + trafficHandled_Frequency,
            success: function (result) {
                var jsonObject = eval(result);
                GetLollyPopChart(jsonObject);
            }
        });
    }
    //Create and call KPI2 L1
    function GetKPI2L1() {
        trafficHandled_Frequency = $('#slotadherence').find(":selected").text();
        $.ajax({
            url: "/Home/GetKPI2SA_L1",
            data: "Frequency=" + trafficHandled_Frequency,
            success: function (result) {
                var jsonObject = eval(result);
                $("#ekID").attr("data-percent", jsonObject[0]['SA_PERCENT']);
                $('#oalID').attr("data-percent", jsonObject[1]['SA_PERCENT']);
                $('#towingID').attr("data-percent", jsonObject[2]['SA_PERCENT']);
                LoadKPI2(jsonObject);
            }
        });
    }

</script>

